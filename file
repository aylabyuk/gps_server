
SITEDETAILS
{
    name: SITENAME (via site_name)
    location: LOCATION 
    marker: String
    other_name: SITENAME (via site_name)
    constructed: Date
    contact_people: [ CONTACT ]
    fileUploads: [ FILEUPLOAD ]
    survey_type: String
}

LOCATION
{
    id: ID
    long: String
    lat: String
    address: String
    description: String
}

FILEUPLOAD
{
    file: FILE
    description: String
}

input FILE {
  name: String!
  type: String!
  size: Int!
  path: String!
}